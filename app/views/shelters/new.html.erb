<form action="/shelters" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>Enter a new shelter:</p>
  Name: </br>
  <input type='text' name='shelter[name]'/><br/></br>
  Address: </br>
  <textarea name='shelter[address]'></textarea><br/></br>
  City: </br>
  <textarea name='shelter[city]'></textarea><br/></br>
  State: </br>
  <textarea name='shelter[state]'></textarea><br/></br>
  Zip Code: </br>
  <textarea name='shelter[zip]'></textarea><br/></br>
  <input type='submit'/>
</form>
